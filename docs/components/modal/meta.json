{
  "title": "Modal",
  "component": "bModal",
  "jsfiddle": "bofh9aaa",
  "events": [
    {
      "event": "change",
      "args": [
        {
          "arg": "is_visible",
          "description": ""
        }
      ],
      "description": "new modal visibility state"
    },
    {
      "event": "shown",
      "description": "always emits when modal is shown"
    },
    {
      "event": "hidden",
      "description": "always emits when modal is hidden",
      "args": {
        "e": "Close event. See docs"
      }
    },
    {
      "event": "ok",
      "description": "when default ok button pressed",
      "args": {
        "e": "Close event. See docs"
      }
    },
    {
      "event": "cancel",
      "description": "when default cancel button pressed",
      "args": {
        "e": "Close event. See docs"
      }
    }
  ],
  "slots": [
    {
      "name": "modal-header",
      "description": "Entire modal header container"
    },
    {
      "name": "modal-title",
      "description": "Modal title"
    },
    {
      "name": "modal-footer",
      "description": "Modal footer content"
    }
  ]
}